#include <stdio.h>
/* Desenvolver uma função que exiba os múltiplos
de N compreendidos no intervalo definido por
A e B. */


    int multiplos (int n, int a, int b)
    {
        int i, multiplo;
 for (i=1; i<=b;i++){
     if(n*i <= b && n*i >= a)
     {
        multiplo = n*i;
         printf("%d ", multiplo);
     }
 }
    }
 void main ()
{	
	
	int numero, inferior, superior;
	
	printf ("Entre com um numero: ");
	scanf ("%d", &numero);
	
	printf ("Entre com o intervalo: ");
	scanf ("%d %d", &inferior, &superior);
	
	multiplos (numero, inferior, superior);
}

    